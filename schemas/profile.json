{
    "type": "jsonschema",
    "schema": {
        "id": "profile",
        "type": "object",
        "properties": {
            "firstName": {
                "id": "firstName",
                "type": "string",
                "pattern": "^w{3,64}"
            },
            "lastName": {
                "id": "lastName",
                "type": "string",
                "pattern": "^w{3,64}"
            },
            "birthday": {
                "id": "birthday",
                "type": "string",
                "pattern": "^(-?(?:[1-9][0-9]*)?[0-9]{4})-(1[0-2]|0[1-9])-(3[01]|0[1-9]|[12][0-9])T(2[0-3]|[01][0-9]):([0-5][0-9]):([0-5][0-9])(.[0-9]+)?(Z|[+-](?:2[0-3]|[01][0-9]):[0-5][0-9])?$"
            },
            "account": {
                "id": "account",
                "type": "string",
                "pattern": "[w]+.[w]+.[w]+"
            },
            "sessions": {
                "id": "sessions",
                "type": "array",
                "items": {
                    "type": "string",
                    "pattern": "[w]+.[w]+.[w]+"
                },
                "uniqueItems": true
            }
        },
        "required": [
            "firstName",
            "lastName",
            "birthday",
            "account"
        ]
    }
}